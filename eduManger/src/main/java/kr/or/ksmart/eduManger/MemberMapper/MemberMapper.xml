<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
	<mapper namespace="kr.or.ksmart.eduManger.MemberMapper">
		<select id="getMemberList"
				resultType="kr.or.ksmart.eduManger.vo.Member">
		SELECT 
			m_id
			, m_AC
			, m_pw
			, m_level
			, m_name
			, m_gender
			, m_birth
			, m_phone
			, m_date
		FROM tb_member
		</select>  
  
    </mapper>